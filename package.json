{
  "name": "spellbook",
  "version": "1.11.1",
  "private": true,
  "dependencies": {
    "classnames": "^2.2.6",
    "history": "^5.0.0",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-markdown": "^4.3.1",
    "react-redux": "^7.2.0",
    "react-scripts": "3.4.1",
    "react-window": "^1.8.5",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "reselect": "^4.0.0",
    "thenby": "^1.3.3"
  },
  "devDependencies": {
    "empty": "^0.10.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint-plugin-prettier": "^3.1.4",
    "firebase-tools": "^8.4.3",
    "flow-bin": "^0.127.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.7",
    "prettier": "^2.0.5",
    "react-test-renderer": "^16.13.1",
    "stylelint": "^13.6.1",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-scss": "^3.17.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy": "firebase deploy",
    "flow": "flow",
    "format:js": "eslint --ext=js,jsx src",
    "format:css": "stylelint src"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json}": [
      "eslint --fix"
    ],
    "src/**/*.{scss}": [
      "stylelint --config=.stylelintrc --fix",
      "prettier --config .prettierrc --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "lint-staged"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
